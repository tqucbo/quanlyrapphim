@model string

@{
    ViewData["Title"] = "Tạo quyền mới";
}

<h1>
    @ViewData["Title"]
</h1>
<hr>

<form action="Create" method="post">
    <div class="form-group row no-gutters">
        <label for="name" class="col-3 col-form-label">Tên quyền</label>
        <input type="text" name="name" id="name" class="form-control col-sm-6">
    </div>
    <button class="btn btn-primary" type="submit">Gửi thông tin</button>
</form>